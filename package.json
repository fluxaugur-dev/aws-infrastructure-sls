{
  "name": "aws-infrastructure-sls",
  "version": "0.0.0",
  "description": "Set up AWS infrastructure using Serverless Framework",
  "dependencies": {
    "-": "0.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.1.1",
    "@semantic-release/npm": "^7.0.6",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@serverless/eslint-config": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "git-list-updated": "^1.2.1",
    "prettier": "^2.1.2"
  },
  "scripts": {
    "lint": "eslint  --ignore-path .gitignore .",
    "lint:updated": "pipe-git-updated --ext=js -- eslint --ignore-path .gitignore",
    "prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettier-check:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier -c",
    "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml}\"",
    "prettify:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier --write"
  },
  "keywords": [
    "alarm",
    "aws",
    "amazon web services",
    "billing",
    "cloudwatch",
    "infrastructure",
    "serverless",
    "serverless framework",
    "simple notification service",
    "sls",
    "sns",
    "topic"
  ],
  "author": {
    "name": "Nathanial L. McConnell",
    "email": "nate@fluxaugur.dev",
    "url": "https://fluxaugur.dev"
  },
  "license": "GPL-3.0",
  "private": true,
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "package-lock.json",
    "README.md"
  ]
}
